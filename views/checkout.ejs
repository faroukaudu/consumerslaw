                                                                                <td class="text-start proName">
                                                                                    <div class="list-view-item-title">
                                                                                        <a href="product-layout1.html"><%= items.name%></a>
                                                                                    </div>
                                                                                    <div class="cart-meta-text">
                                                                                        Ebook
                                                                                    </div>
                                                                                </td>
                                                                                <td class="text-center">1</td>
                                                                                <td class="text-center">$<%= items.amount%>.00</td>
                                                                                <td class="text-center"><strong>$<%= items.amount%>.00</strong></td>
                                                                            </tr>

                                                                        <% }) %>
                                                                    <% } %>
                                                                    
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
